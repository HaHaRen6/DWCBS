            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        },
        {
            "name": "Run CBS",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run_experiments.py",
            "console": "integratedTerminal",
            "args": [
                "--instance", "instances/exp0.txt",
                "--solver", "CBS"
            ],
            "python": "/home/haharen/miniconda3/envs/mapf/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
